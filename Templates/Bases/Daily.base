properties:
  file.name:
    displayName: Date
  note.created:
    displayName: Created
views:
  - type: table
    name: Daily Notes
    filters:
      and:
        - list(categories).contains(link("daily"))
        - '!file.name.contains("Template")'
    order:
      - file.name
      - created
    sort:
      - property: file.name
        direction: DESC
    columnSize:
      file.name: 200
      note.created: 150
