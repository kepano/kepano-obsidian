properties:
  created:
    displayName: Created
  file.name:
    displayName: Filename
  categories:
    displayName: Categories
  author:
    displayName: Author
  topics:
    displayName: Topics
views:
  - type: table
    name: Backlinks
    filters:
      and:
        - file.hasLink(this)
    order:
      - created
      - file.name
      - categories
      - author
      - topics
    sort:
      - property: created
        direction: DESC
    columnSize:
      file.name: 205
